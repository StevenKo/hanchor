<h5> Registration </h5>

<% render 'shared/error', obj: @user %>

<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.label :time_zone %>
  <%= f.time_zone_select :time_zone %>
  <br/>
  <%= f.submit "Register", class: 'btn'%>
<% end %>