  `id` int(11) NOT NULL AUTO_INCREMENT,
  `shipping_address` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
  `billing_address` varchar(255) COLLATE utf8_unicode_ci DEFAULT NULL,
